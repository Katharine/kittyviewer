<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater
 can_resize="false"
 can_dock="true"
 can_minimize="true"
 can_close="false"
 height="73"
 layout="topleft"
 name="floater_quick_settings"
 help_topic="floater_quick_settings"
 title="QUICK SETTINGS"
 save_dock_state="true"
 save_visibility="true"
 save_rect="true"
 single_instance="true"
 width="232">
  <layout_stack
   clip="false"
   follows="all"
   height="20"
   layout="topleft"
   left="10"
   mouse_opaque="false"
   name="my_call_stack"
   orientation="vertical"
   width="252">
    <layout_panel
     follows="top|left|right"
     user_resize="false"
     auto_resize="false"
     layout="topleft"
     height="20"
     name="farclip_panel">
      <slider
       control_name="RenderFarClip"
       decimal_digits="0"
       follows="left|top"
       height="16"
       increment="8"
       initial_value="160"
       label="Draw distance:"
       label_width="75"
       layout="topleft"
       max_val="1024"
       min_val="32"
       name="draw_distance_slider"
       top="3"
       width="230" />
    </layout_panel>
  </layout_stack>
  <text
   top_pad="10"
   left="10"
   layout="topleft"
   width="130">
    Viewer profile:
  </text>
  <combo_box
   follows="left|top|right"
   top_delta="12"
   height="23"
   layout="topleft"
   left="8"
   name="profiles_combo"
   width="150" />
  <button
   height="23"
   label="New"
   label_selected="New"
   layout="topleft"
   left_pad="5"
   name="new_profile_btn"
   width="60" />
</floater>
